  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "pnpm exec jest",
    "test:watch": "pnpm exec jest --watch",
    "test:cov": "pnpm exec jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "pnpm exec jest --config ./test/jest-e2e.json",
    "db:migration:generate": "pnpm exec typeorm-ts-node-commonjs migration:generate src/shared/infrastructure/persistence/typeorm/migrations/SchemaSync -d src/shared/infrastructure/persistence/typeorm/data-source.ts",
    "db:migration:run": "pnpm exec typeorm-ts-node-commonjs migration:run -d src/shared/infrastructure/persistence/typeorm/data-source.ts",
    "db:migration:revert": "pnpm exec typeorm-ts-node-commonjs migration:revert -d src/shared/infrastructure/persistence/typeorm/data-source.ts",
    "db:seed": "pnpm exec ts-node -r tsconfig-paths/register src/shared/infrastructure/persistence/typeorm/seeds/run.ts",
    "db:seed:run": "pnpm exec ts-node -r tsconfig-paths/register src/shared/infrastructure/persistence/typeorm/seeds/run.ts",
    "db:setup": "pnpm db:migration:run && pnpm db:seed"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^8.0.0",
    "@nestjs/throttler": "^6.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "mysql2": "^3.11.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
