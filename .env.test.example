# E2E test environment
# Copy to .env.test and set values for MySQL test DB + test user

# Server (optional for E2E)
PORT=3000
NODE_ENV=test

# Database (MySQL test database - do not use production)
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASS=
DB_NAME=m_order_system_test
DB_LOGGING=false

# JWT (same as dev for signing)
JWT_SECRET=change-me-in-production
JWT_ACCESS_EXPIRY=15m
JWT_REFRESH_SECRET=
JWT_REFRESH_EXPIRY=7d

# E2E: existing user that can login (must exist in test DB)
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=password123
TEST_MERCHANT_ID=your-merchant-uuid
